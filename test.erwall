func fib: (let n: Int32) -> Int32
{
	if(n == 0)
	{
		return 0;
	}
	elseif(n == 1 || n == 2)
	{
		return 1;
	}
	else
	{
		return fib(n - 1) + fib(n - 2);
	}
}

